---
slug: contributors
searchable: false
metadata:
  title: Contributors
  description: Contributors
  questions: false
  contributors: false
---

{%- graphql api = 'api_call', template: 'get_all_contributors_from_github' -%}
{%- assign response = api.api_call_send.response.body | to_hash -%}
{%- assign authors = response | map: "author" -%}

<ul>
{% for author in authors %}
  <li><img src="{{author.avatar_url}}" width="120px" height="120px"/>{{author.login}}</li>
{% endfor %}
</ul>