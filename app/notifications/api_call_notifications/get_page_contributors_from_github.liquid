---
name: get_page_contributors_from_github
delay: '1'
enabled: true
format: http
headers: '{ "Content-Type": "application/json" }'
request_type: GET
to: |-
  {%- graphql g = "get_github_clients" -%}
  {% assign client = g.models.results.last %}
  https://{{ client.username }}:{{ client.token }}@api.github.com/repos/mdyd-dev/nearme-documentation/commits?path={{ data.path }}
trigger_condition: true
---
{}
