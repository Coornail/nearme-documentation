---
layout_name: empty
---
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="x-ua-compatible" content="ie=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link type="text/css" rel="stylesheet" href="{{ 'modules/graphql/graphdoc.css' | asset_url }}" />
  <link type="text/css" rel="stylesheet" href="{{ 'modules/graphql/code.css' | asset_url }}" />
  <link type="text/css" rel="stylesheet" href="{{ 'modules/graphql/require-by.css' | asset_url }}" />
  <title><% title %> | platformOS Documentation</title>
</head>
<body class="slds-scrollable--y">
    <nav class="slds-grid slds-grid--vertical slds-col--rule-right">
        <header class="slds-p-around--medium slds-col slds-shrik slds-grow-none">
            <button class="slds-button slds-button--icon slds-float--right js-toggle-navigation less-than-medium">
                <i class="material-icons">close</i>
            </button>
            <h3 class="slds-text-heading--medium">
              <a href="/" class="logo">
                <img src="/assets/images/svg/logo.svg" width="185" alt="platformOS Logo">
                <span>Docs</span>
              </a>
            </h3>
            <div class="slds-p-top--large">
                <input id="type-search" type="text" placeholder="Search a type" autofocus="" class="slds-input" />
            </div>
        </header>
        <div id="navication-scroll" class="slds-scrollable--y slds-col slds-grow">
            <%> nav %>
        </div>
    </nav>

    {% raw %}
    <main><%> main %></main>
    {% endraw %}

    <script src="{{ 'modules/graphql/focus-active.js' | asset_url }}"></script>
    <script src="{{ 'modules/graphql/filter-types.js' | asset_url }}"></script>
    <script src="{{ 'modules/graphql/toggle-navigation.js' | asset_url }}"></script>
    <script src="{{ 'modules/graphql/line-link.js' | asset_url }}"></script>
</body>
